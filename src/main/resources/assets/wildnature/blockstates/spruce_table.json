{
    "multipart": [
        {   "when": { "north": "true" },
            "apply": { "model": "wildnature:block/spruce_table_leg", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "wildnature:block/spruce_table_leg", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "wildnature:block/spruce_table_leg", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "wildnature:block/spruce_table_leg", "y": 270, "uvlock": true }
        },
        {   "when": { "waterlogged": "true"},
            "apply": { "model": "wildnature:block/spruce_table_top"}
        },
        {   "when": { "waterlogged": "false"},
            "apply": { "model": "wildnature:block/spruce_table_top"}
        }
    ]
}
